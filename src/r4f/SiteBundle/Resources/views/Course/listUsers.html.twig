<h6>Participants :</h6>
<ul class="users">
{% for user in users %}
    <li>
        <a href="{{ path('r4f_runner_user_select', { 'id': user.id  }) }}" data-placement="bottom" rel="tooltip" title="{{ user.username }}<br />{{ user.email }}" class="user-tooltip">
        <i class="icon-user"> </i> {{ user.username }} </a>
    </li>
{% endfor %}
</ul>
