<?php

namespace r4f\CourseBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * @ORM\Entity
 */
class Address
{
   /**
    * @ORM\Id
    * @ORM\Column(type="integer")
    * @ORM\GeneratedValue(strategy="AUTO")
    */
    protected $id;

    /**
     * @ORM\Column(type="string", length=255)
     */
    private $address;
	
    /**
     * @ORM\Column(type="string", length=255)
     */
    private $zip_code;
	
    /**
     * @ORM\Column(type="string", length=255)
     */
    private $city;
	
    /**
     * @ORM\Column(type="string", length=255)
     */
    private $country;
	
    /**
     * @ORM\Column(type="float")
     */
    private $latitude;
	
    /**
     * @ORM\Column(type="float")
     */
    private $longitude;
	
}